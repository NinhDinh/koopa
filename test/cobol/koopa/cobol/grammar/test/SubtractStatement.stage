package koopa.cobol.grammar.test;
grammar koopa.cobol.grammar.CobolGrammar;

target subtraction_format1;

+[ CORRESPONDING A FROM B ]
+[ CORR A FROM B ]

+[ CORRESPONDING A FROM B ROUNDED ]
+[ CORR A FROM B ROUNDED ]

target subtraction_format2;

+[ A FROM B GIVING C ]
+[ A FROM B GIVING C ROUNDED ]

+[ A B FROM C GIVING D E ]
+[ A B FROM C GIVING D ROUNDED E ROUNDED ]

+[ A B GIVING C ]
+[ A B GIVING C ROUNDED ]

+[ A B C GIVING D E ]
+[ A B C GIVING D ROUNDED E ROUNDED ]

+[ ZERO FROM B GIVING C ]
+[ A FROM ZERO GIVING C ]

target subtraction_format3;

+[ A FROM B ]
+[ A FROM B ROUNDED ]

+[ A B FROM C D ]
+[ A B FROM C ROUNDED D ROUNDED ]

+[ A ZERO FROM C D ]

target subtractStatement;

+[ SUBTRACT A FROM B # . ]

+[ SUBTRACT A FROM B GIVING C # . ]

+[ SUBTRACT A FROM B
     ON SIZE ERROR
        DISPLAY "OOPS" ]

+[ SUBTRACT A FROM B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS" ]

+[ SUBTRACT A FROM B
     SIZE ERROR
        DISPLAY "OOPS" ]

+[ SUBTRACT A FROM B GIVING C
     SIZE ERROR
        DISPLAY "OOPS" ]

+[ SUBTRACT A FROM B
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ SUBTRACT A FROM B GIVING C
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ SUBTRACT A FROM B
     NOT SIZE ERROR
        DISPLAY "AOK" ]

+[ SUBTRACT A FROM B GIVING C
     NOT SIZE ERROR
        DISPLAY "AOK" ]

+[ SUBTRACT A FROM B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ SUBTRACT A FROM B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ SUBTRACT A FROM B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-SUBTRACT ]

+[ SUBTRACT A FROM B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-SUBTRACT ]

+[ SUBTRACT A FROM B
   END-SUBTRACT ]

+[ SUBTRACT A FROM B GIVING C
   END-SUBTRACT ]
