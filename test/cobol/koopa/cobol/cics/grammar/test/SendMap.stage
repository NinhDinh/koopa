package koopa.cobol.cics.grammar.test;
grammar koopa.cobol.cics.grammar.CICSGrammar;
tokenizer koopa.cobol.sources.test.CobolTestSource;

target cicsSendMap;


# Based on https://sites.google.com/site/cicssampleprograms1/home

+[ SEND MAP('FILE')                    
     MAPSET('FILE01') ]

+[ SEND MAP('FILE1')
     MAPSET('FILE02')              
     ERASE ]


# Based on http://www.csis.ysu.edu/~tom/crs/cis4808/SENDMAP.txt

+[ SEND MAP ('ZnnAM01')
     FROM (ZnnAM01O)
     MAPSET ('ZnnAMAP')
     ERASE ]


# Based on http://infocenter.sybase.com/archive/index.jsp?topic=/com.sybase.infocenter.dc36123.1502/html/XA/X21092.htm

+[ SEND MAP('MSGLINE') 
     MAPSET('AIXCSET')
     FREEKB ]
