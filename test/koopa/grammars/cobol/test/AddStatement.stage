package koopa.grammars.cobol.test;
grammar koopa.grammars.cobol.CobolGrammar;

target addStatement;

+[ ADD A TO B ¥ . ]

+[ ADD A TO B GIVING C ¥ . ]

+[ ADD A TO B
     ON SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ ADD A TO B GIVING C
     ON SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ ADD A TO B
     SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ ADD A TO B GIVING C
     SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ ADD A TO B
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ ADD A TO B GIVING C
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ ADD A TO B
     NOT SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ ADD A TO B GIVING C
     NOT SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ ADD A TO B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ ADD A TO B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ ADD A TO B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-ADD ]

+[ ADD A TO B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-ADD ]

+[ ADD A TO B
   END-ADD ]

+[ ADD A TO B GIVING C
   END-ADD ]
