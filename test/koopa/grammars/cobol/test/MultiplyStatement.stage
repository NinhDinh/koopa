package koopa.grammars.cobol.test;
grammar koopa.grammars.cobol.CobolGrammar;

target multiplication_format1;

+[ A BY B GIVING C ]
+[ A BY B GIVING C D ]

+[ A BY B GIVING C ROUNDED ]
+[ A BY B GIVING C ROUNDED D ROUNDED ]

target multiplication_format2;

+[ A BY B ]
+[ A BY B C ]

+[ A BY B ROUNDED ]
+[ A BY B ROUNDED C ROUNDED ]

target multiplyStatement;

+[ MULTIPLY A BY B # . ]

+[ MULTIPLY A BY B GIVING C # . ]

+[ MULTIPLY A BY B
     ON SIZE ERROR
        DISPLAY "OOPS" ]

+[ MULTIPLY A BY B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS" ]

+[ MULTIPLY A BY B
     SIZE ERROR
        DISPLAY "OOPS" ]

+[ MULTIPLY A BY B GIVING C
     SIZE ERROR
        DISPLAY "OOPS" ]

+[ MULTIPLY A BY B
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ MULTIPLY A BY B GIVING C
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ MULTIPLY A BY B
     NOT SIZE ERROR
        DISPLAY "AOK" ]

+[ MULTIPLY A BY B GIVING C
     NOT SIZE ERROR
        DISPLAY "AOK" ]

+[ MULTIPLY A BY B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ MULTIPLY A BY B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK" ]

+[ MULTIPLY A BY B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-MULTIPLY ]

+[ MULTIPLY A BY B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-MULTIPLY ]

+[ MULTIPLY A BY B
   END-MULTIPLY ]

+[ MULTIPLY A BY B GIVING C
   END-MULTIPLY ]
