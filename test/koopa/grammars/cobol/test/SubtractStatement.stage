package koopa.grammars.cobol.test;
grammar koopa.grammars.cobol.CobolGrammar;

target subtractStatement;

+[ SUBTRACT A FROM B ¥ . ]

+[ SUBTRACT A FROM B GIVING C ¥ . ]

+[ SUBTRACT A FROM B
     ON SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ SUBTRACT A FROM B GIVING C
     ON SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ SUBTRACT A FROM B
     SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ SUBTRACT A FROM B GIVING C
     SIZE ERROR
        DISPLAY ¥ "OOPS" ]

+[ SUBTRACT A FROM B
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ SUBTRACT A FROM B GIVING C
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ SUBTRACT A FROM B
     NOT SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ SUBTRACT A FROM B GIVING C
     NOT SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ SUBTRACT A FROM B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ SUBTRACT A FROM B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY ¥ "AOK" ]

+[ SUBTRACT A FROM B
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-SUBTRACT ]

+[ SUBTRACT A FROM B GIVING C
     ON SIZE ERROR
        DISPLAY "OOPS"
     NOT ON SIZE ERROR
        DISPLAY "AOK"
   END-SUBTRACT ]

+[ SUBTRACT A FROM B
   END-SUBTRACT ]

+[ SUBTRACT A FROM B GIVING C
   END-SUBTRACT ]
